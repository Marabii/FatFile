{
  "name": "fatfile",
  "displayName": "FatFile: Ultra-Fast Large Log Viewer",
  "description": "Open 10GB+ logs instantly with O(1) random access. High-performance streaming powered by a Rust backend.",
  "version": "0.1.0",
  "publisher": "marabii",
  "author": "Hamza DADDA",
  "license": "SEE LICENSE IN LICENSE",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marabii/FatFile"
  },
  "bugs": {
    "url": "https://github.com/Marabii/FatFile/issues"
  },
  "homepage": "https://github.com/Marabii/FatFile#readme",
  "engines": {
    "vscode": "^1.90.0"
  },
  "keywords": [
    "logs",
    "viewer",
    "performance",
    "rust",
    "streaming",
    "large files",
    "tail"
  ],
  "extensionKind": [
    "workspace",
    "ui"
  ],
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "fatfile.viewer",
        "displayName": "FatFile Viewer",
        "selector": [
          {
            "filenamePattern": "*.log"
          },
          {
            "filenamePattern": "*.txt"
          }
        ],
        "priority": "option"
      }
    ],
    "commands": [
      {
        "command": "fatfile.openWithViewer",
        "title": "Open with FatFile Viewer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-window": "^1.8.8",
    "@types/vscode": "^1.90.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "autoprefixer": "^10.4.16",
    "css-loader": "^6.8.1",
    "eslint": "^9.39.1",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.4",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-window": "^1.8.10"
  }
}
